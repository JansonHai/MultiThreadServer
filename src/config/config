#配置说明
#server_port 服务器监听端口,默认：6666
#log_path log的目录, 默认: ./server.log
#max_database_connection 数据库连接池的最大连接数，默认：256
#max_client_connection 最大客户端连接数，默认：2048
#read_thread_num 数据读取线程，默认16
#work_thread_num 工作线程数，默认256
#message_max_length 客户端数据包最大长度,默认2M，即2097152

server_port = 6666
log_path = ./log/server.log
max_database_connection = 256
max_client_connection = 2048
read_thread_num = 16
work_thread_num = 16
message_max_length = 2097152    #2M
